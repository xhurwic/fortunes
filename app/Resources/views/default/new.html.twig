{% extends 'base.html.twig' %}

{% block body %}

        <h1>Create a new fortune</h1>
       <form action=" {{ path('create') }}" method = "post" novalidate>
               {{ form_row(form.title) }}
               {{ form_row(form.author) }}
               {{ form_row(form.content) }}
               {{ form_row(form.submit) }}
               {{ form_rest(form) }}
       </form>
{% endblock %}